<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:HTMLPanel styleName="tt-popup">
		<div class="tt-confirmationWindow">
			<div class="row">
				<div class="twelve columns tt-label tt-underline" style="margin-bottom: 2em;">
					<g:Label ui:field="titleLabel">Tag Name Editor</g:Label>
				</div>
			</div>
			<div class="row"  style="margin-bottom: 2em; max-height: 30em; overflow: auto;">
				<div class="twelve columns">
					<g:HTMLPanel ui:field="tagNamesPanel"></g:HTMLPanel>
					<g:Anchor ui:field="createTagAnchor" />
				</div>
			</div>	
			
			<div class="row">
				<div class="twelve columns">
					<g:Anchor ui:field="addNewTag">add new</g:Anchor>
				</div>
			</div>
			
			<g:HTMLPanel styleName="row" ui:field="okOnlyPanel">
				<div class="center" style="width: 7em;">
					<g:Button styleName="button-primary" ui:field="okButton">Close</g:Button>
				</div>
			</g:HTMLPanel>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 