<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:HTMLPanel styleName="tt-popup">
		<div class="tt-confirmationWindow">
			<div class="row">
				<div class="twelve columns tt-label tt-underline" style="margin-bottom: 2em;">
					<g:Label ui:field="titleLabel">Delete Confirmation</g:Label>
				</div>
			</div>
			<div class="row"  style="margin-bottom: 2em;">
				<div class="twelve columns">
					<g:Label ui:field="messageLabel"></g:Label>
				</div>
			</div>	
			<g:HTMLPanel styleName="row" ui:field="okCancelPanel">
				<div class="center" style="width: 15em;">
					<g:Button ui:field="okButton">Ok</g:Button>
					<g:Button styleName="button-primary" ui:field="cancelButton">Cancel</g:Button>
				</div>
			</g:HTMLPanel>
			<g:HTMLPanel styleName="row" ui:field="okOnlyPanel">
				<div class="center" style="width: 7em;">
					<g:Button styleName="button-primary" ui:field="onOnlyButton">Ok</g:Button>
				</div>
			</g:HTMLPanel>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 