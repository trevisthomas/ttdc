<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>
	<g:HTMLPanel styleName="row">
		<div class="twelve colums, tt-padded">
			<div class="row">
			    <div class="six columns">
			      <label for="termInput">Term</label>
			      <g:TextBox styleName="u-full-width" ui:field="termTextBox"/>
			    </div>
		
			    <div class="six columns">
			     	<label for="definitionInput">Definition</label>
					<g:TextBox styleName="u-full-width" ui:field="definitionTextBox"/>
			    </div>
		    </div>
		    <div class="row">
		    	<div class="twelve columns tt-lookupLinkFix">
	    			<g:Anchor ui:field="spanishDictButton">spanish dict</g:Anchor> | <g:Anchor ui:field="lingueeButton">linguee</g:Anchor>
		    	</div>
		    </div>
	    	<div class="row">
		    	<div class="ten columns">
					<g:Button styleName="tt-btn-fix" ui:field="deleteButton"/><g:Button styleName="tt-btn-fix" ui:field="activateButton"/><g:Button styleName="tt-btn-fix" ui:field="deactivateButton"/>
				</div>
				<div class="two columns">
					<g:Button styleName="button-primary" ui:field="closeButton"/>
				</div>
			</div>
			
			<div class="row">
				<div class="twelve columns">
					<g:HTMLPanel ui:field="tagMePanel"></g:HTMLPanel>
				</div>
			</div>
		</div>
		
	</g:HTMLPanel>
</ui:UiBinder> 