<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:HTMLPanel ui:field="mainHtmlPanel">
		<g:Grid ui:field="wraperGrid" styleName="cardView">
			<g:row styleName="optionalHeaderStyle">
				<g:customCell styleName="optionalFooCellStyle">
					<g:Label ui:field="numberLabel" />
				</g:customCell>
				<g:customCell styleName="optionalBarCellStyle">
					<g:VerticalPanel>
							<g:HorizontalPanel>
								<g:Label ui:field="termLabel" />
								<g:Label ui:field="tagsLabel" />
							</g:HorizontalPanel>
							<g:Label ui:field="definitionLabel" />
						</g:VerticalPanel>
				</g:customCell>
			</g:row>
			
		</g:Grid>
	</g:HTMLPanel>
</ui:UiBinder> 