<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<!-- The above doctype is to allow for special html characters in this xml file -->
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
    xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	 <g:HTMLPanel>
	 	<table>
	 		<tr>
	 			<td>
	 				<table class="tt-fill">
			 			<tr class="tt-fill">
			 				<td><g:FocusPanel ui:field="prevMonthElement"/></td>
			 				<td class="tt-center tt-fill"><g:Hyperlink ui:field="monthTitleElement"/><g:Hyperlink ui:field="yearTitleElement"/></td>
			 				<td><g:FocusPanel ui:field="nextMonthElement"/></td>
			 			</tr>
			 		</table>
	 			</td>
	 		</tr>
	 		<tr>
	 			<td>
	 				<g:Grid styleName="tt-fill tt-calendar-small-month" ui:field="daysGridElement"/>			
	 			</td>
	 		</tr>
	 		<tr>
	 			<td class="tt-center">
	 				<g:Button ui:field="clearButtonElement"/>
	 				<g:Button ui:field="nowButtonElement"/>
	 			</td>
	 		</tr>
	 		
	 	</table>
	 </g:HTMLPanel>
</ui:UiBinder>


