<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<!-- The above doctype is to allow for special html characters in this xml file -->


<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
    xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	 <g:HTMLPanel styleName="tt-pageHeader">
	 	<g:Widget styleName="tt-text-center tt-text-small" ui:field="navigationElement" ></g:Widget>
	 	<div>
	 		<div class="tt-float-left">
	 			<g:Label styleName="tt-pageHeader-title tt-text-huge" ui:field="titleElement"></g:Label>
	 			<g:Label styleName="tt-pageHeader-subtitle" ui:field="subTitleElement"></g:Label>
	 		</div>
	 		<div class="tt-float-right">
	 			<g:Widget styleName="tt-text-small" ui:field="loginElement"></g:Widget>
	 	 		<g:Widget ui:field="searchElement" ></g:Widget>
	 	 	</div>
		</div>
	 </g:HTMLPanel>
</ui:UiBinder>