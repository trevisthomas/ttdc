<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<!-- The above doctype is to allow for special html characters in this xml file -->
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
    xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	 <g:HTMLPanel styleName="tt-text-normal ">
	 	<g:Widget ui:field="pageHeaderElement"/>
	 	<div class="tt-search-results">
		 	<g:Label ui:field="searchSummaryDetailElement" styleName="tt-text-large-bold tt-text-center"/>
		 	
		 	
		 	<g:Label styleName="tt-text-center" ui:field="pageResultMessageElement"/>
		 	<div class="tt-post-container-header tt-center">
		 		<div class="tt-float-left">
		 			sort by: <g:Hyperlink ui:field="sortByDateElement"/> | <g:Hyperlink ui:field="sortByRelevanceElement"/>
		 		</div>
		 		<g:Widget ui:field="paginationNanoElement"/>
		 	</div>
		 	<g:SimplePanel styleName="tt-fill" ui:field="postListElement"/>
		 	<div class="tt-post-container-footer tt-center">
		 		<g:Widget ui:field="paginationElement"/>
		 	</div>
		 	<!-- 
		 	<g:Hyperlink styleName="tt-fill tt-text-left" ui:field="expandSearchResultsElement"/>
		 	 -->
	 	</div>
	 	<g:Widget ui:field="pageFooterElement"/>
	 </g:HTMLPanel>
</ui:UiBinder>

	