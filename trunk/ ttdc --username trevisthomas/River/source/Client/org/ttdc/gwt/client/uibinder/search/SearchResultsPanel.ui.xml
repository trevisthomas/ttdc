<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<!-- The above doctype is to allow for special html characters in this xml file -->
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
    xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	 <g:HTMLPanel styleName="tt-text-normal ">
	 	<g:Widget styleName="tt-fill" ui:field="pageHeaderElement"/>
	 	<div class="tt-search-results">
		 	<g:Label ui:field="searchSummaryDetailElement" styleName="tt-text-large-bold tt-text-center"/>
		 	<g:Label styleName="tt-fill tt-text-large-bold tt-text-left tt-color-search-results-header" ui:field="tagLabelElement"/>
		 	<g:SimplePanel styleName="tt-fill" ui:field="tagResultsElement"/>
		 	<g:Label styleName="tt-fill tt-text-large-bold tt-text-left tt-color-search-results-header" ui:field="postLabelElement"/>
		 	<div class="tt-post-container-header tt-center">
		 		<g:Label styleName="tt-float-left" ui:field="pageResultMessageElement"/>
		 		<div class="tt-float-right">
		 			<g:Hyperlink ui:field="prevElement"/> &nbsp;&nbsp; <g:Hyperlink ui:field="nextElement"/>
		 		</div>
		 	</div>
		 	<g:SimplePanel styleName="tt-fill" ui:field="postListElement"/>
		 	<div class="tt-post-container-footer tt-center">
		 		<g:Widget styleName="tt-center" ui:field="paginationElement"/>
		 	</div>
		 	<g:Hyperlink styleName="tt-fill tt-text-left" ui:field="expandSearchResultsElement"/>
	 	</div>
	 </g:HTMLPanel>
</ui:UiBinder>

	