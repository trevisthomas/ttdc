<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<!-- The above doctype is to allow for special html characters in this xml file -->
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	
	<g:HTMLPanel>
		<g:Widget ui:field="pageHeaderElement"/>
		<g:SimplePanel ui:field="siteUpdateElement"/>
		<table class="tt-split-container">
			<tr>
				<td class="tt-split-home-main">
					<g:TabPanel ui:field="postTabPanelElement" />					
				</td>
				<td class="tt-split-home-right">
					<g:VerticalPanel ui:field="sidebarElement" />
				</td>
			</tr>
		</table>
		<g:Widget ui:field="pageFooterElement"/>
	</g:HTMLPanel>
</ui:UiBinder>


