<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<!-- The above doctype is to allow for special html characters in this xml file -->
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	xmlns:t='urn:import:org.ttdc.gwt.client.presenters.util'>
	
	<g:HTMLPanel>
		<g:Widget ui:field="pageHeaderElement"/>
		<div>
			<div class="tt-forum-names">
				<g:Label styleName="tt-fill tt-text-large-bold tt-text-left tt-color-search-results-header" ui:field="postLabelElement"/>
				<t:UnorderedListWidget ui:field="forumsElement"/>
			</div>
			<g:TabPanel styleName="tt-forum-topics" ui:field="topicTabPanelElement" />
		</div>
		<g:Widget ui:field="pageFooterElement" />		
	</g:HTMLPanel>
</ui:UiBinder>
