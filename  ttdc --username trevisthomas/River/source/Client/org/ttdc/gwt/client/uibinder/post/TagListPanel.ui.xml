<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<!-- The above doctype is to allow for special html characters in this xml file -->
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	<g:HTMLPanel styleName="tt-fill tt-color-tagList">
			<g:HTMLPanel ui:field="tagListElement" styleName="tt-text-small">
				<div class="tt-inline">Tags:</div>
				<g:FlowPanel styleName="tt-inline" ui:field="tagsElement">
				</g:FlowPanel>
				<!-- 
				<g:HTMLPanel ui:field="tagEditElement">
					(<g:Anchor ui:field="editTagsLinkElement" styleName="tt-cursor-pointer"/>)  
				</g:HTMLPanel>
				-->
				
			</g:HTMLPanel>
			<g:HTMLPanel ui:field="editableTagListElement">
				<table class="tt-text-normal">
					<tr>
						<td><g:FlowPanel ui:field="editableTagsElement"/></td>
						<td><g:SimplePanel ui:field="tagSelectorElement"/></td>
						<td><g:Button ui:field="addButtonElement"/><g:Button ui:field="cancelButtonElement"/></td>
					</tr>
					<tr>
						<td colspan="3">
							<g:CheckBox ui:field="nwsCheckBoxElement">NWS</g:CheckBox>
							<g:CheckBox ui:field="infCheckBoxElement">Informative</g:CheckBox>
							<g:CheckBox ui:field="privateCheckBoxElement">Private</g:CheckBox>
							<g:CheckBox ui:field="lockedCheckBoxElement">Locked</g:CheckBox>
							<g:CheckBox ui:field="deletedCheckBoxElement">Deleted</g:CheckBox>
							<g:CheckBox ui:field="reviewCheckBoxElement">Review</g:CheckBox>
						</td>
					</tr>
				</table>
			</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder>
